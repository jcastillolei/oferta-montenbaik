  "image#noticia-1": {
    "props": {
      "src": "https://montenbaikcl.vtexassets.com/assets/vtex.file-manager-graphql/images/20e23a4e-7c98-48f7-a75d-20382a106b6f___6c07dc850295eee7ad7047e7e295c0a8.jpg",
