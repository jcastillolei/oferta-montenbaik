{
  "footer": {
    "blocks": [
      "footer-layout.desktop",
      "footer-layout.mobile"
    ]
  },
  "footer-layout.desktop": {
    "children": [
      "flex-layout.row#footer#rrss",
      "flex-layout.row#footer-newsletter",
      "flex-layout.row#footer-general",
      "flex-layout.row#footer-bottom"
    ]
  },
  "flex-layout.row#footer-general": {
    "children": [
      "flex-layout.col#footer-general"
    ],
    "props": {
      "blockClass": "footer-general"
    }
  },
  "flex-layout.row#footer#rrss": {
    "children": [
      "rich-text#footer-rrss",
      "image#rrss-instagram",
      "image#rrss-youtube",
      "image#rrss-facebook"
    ],
    "props": {
      "horizontalAlign": "center",
      "preventHorizontalStretch": true,
      "fullWidth": true,
      "blockClass": "footer-rrss"
    }
  },
  "rich-text#footer-rrss": {
    "props": {
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "text": "SÍGUENOS EN: ",
      "blockClass": "titulo-rrss"
    }
  },
  "image#rrss-instagram": {
    "props": {
      "src": "/arquivos/icono-instagram.png",
      "width": 40,
      "link": {
        "url": "https://www.instagram.com/montenbaik/"
      },
      "blockClass": "imagen-rrss"
    }
  },
  "image#rrss-youtube": {
    "props": {
      "src": "/arquivos/icono-youtube.png",
      "width": 40,
      "link": {
        "url": "https://www.youtube.com/user/solmini"
      },
      "blockClass": "imagen-rrss"
    }
  },
  "image#rrss-facebook": {
    "props": {
      "src": "/arquivos/icono-facebook.png",
      "width": 40,
      "link": {
        "url": "https://www.facebook.com/Montenbaik/"
      },
      "blockClass": "imagen-rrss"
    }
  },
  "flex-layout.row#footer-newsletter": {
    "children": [
      "rich-text#Newsletter",
      "flex-layout.row#formNewsletter"
    ],
    "props": {
      "horizontalAlign": "center",
      "preventHorizontalStretch": true,
      "fullWidth": true,
      "blockClass": "footer-newsletter"
    }
  },
  "rich-text#Newsletter": {
    "props": {
      "textPosition": "LEFT",
      "text": "**Suscribete** \n y Recibiras las ultimas\n noticias y ofertas!",
      "blockClass": "titulo-newsletter"
    }
  },
  "flex-layout.row#formNewsletter": {
    "children": [
      "flex-layout.col#formNewsletter"
    ],
    "props": {
      "blockClass": "row-newsletter"
    }
  },
  "flex-layout.col#formNewsletter": {
    "children": [
      "newsletter"
    ],
    "props": {
      "blockClass": "form-newsletter"
    }
  },
  "newsletter": {
    "props": {
      "hideLabel": true,
      "submit": ">",
      "placeholder": "Ingresa tu e-mail",
      "blockClass": "newsletter"
    }
  },
  "flex-layout.col#footer-general": {
    "children": [
      "flex-layout.row#footer-general-fila-1"
    ],
    "props": {
      "blockClass": "col-footer-general"
    }
  },
  "flex-layout.row#footer-general-fila-1": {
    "children": [
      "flex-layout.col#footer-general-item-1",
      "flex-layout.col#footer-general-item-2",
      "flex-layout.col#footer-general-item-3",
      "flex-layout.col#footer-general-item-4",
      "flex-layout.col#footer-general-item-5"
    ],
    "props": {
      "blockClass": "footer-general-fila-1",
      "preventHorizontalStretch": true
    }
  },
  "flex-layout.col#footer-general-item-1": {
    "children": [
      "image#logo-footer",
