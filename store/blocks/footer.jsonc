      "menu-item#footer-telefono",
      "menu-item#footer-email"
    ],
    "props": {
      "blockClass": "col-item-footer"
    }
  },
  "image#logo-footer": {
    "props": {
      "src": "/arquivos/logo.svg",
      "width": 200,
      "blockClass": "logo-footer"
    }
  },
